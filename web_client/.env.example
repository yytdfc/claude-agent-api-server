# API Mode Configuration
# Set to 'true' to use unified /invocations endpoint, 'false' for direct REST API
VITE_USE_INVOCATIONS=true

# Terminal Output Mode
# Set to 'true' for SSE streaming (HTTP/2), 'false' for HTTP polling
VITE_TERMINAL_USE_STREAMING=true

# AWS Cognito Configuration
# User authentication and registration settings
VITE_COGNITO_REGION=us-west-2
VITE_COGNITO_USER_POOL_ID=your-user-pool-id
VITE_COGNITO_CLIENT_ID=your-client-id

# Optional: Cognito OAuth domain for hosted UI
# VITE_COGNITO_OAUTH_DOMAIN=your-domain.auth.us-west-2.amazoncognito.com

# Default Settings (can be overridden in UI Settings)
# Backend API server URL
VITE_DEFAULT_SERVER_URL=http://127.0.0.1:8000

# Default working directory
VITE_DEFAULT_CWD=/workspace

# Default Claude model for main responses
VITE_DEFAULT_MODEL=global.anthropic.claude-sonnet-4-5-20250929-v1:0

# Default background model for quick tasks
VITE_DEFAULT_BACKGROUND_MODEL=global.anthropic.claude-haiku-4-5-20251001-v1:0

# Enable LiteLLM proxy by default (set to 'true' to enable)
VITE_DEFAULT_ENABLE_PROXY=false

# UI Configuration
# Hide the settings button in the header (set to 'true' to hide)
VITE_HIDE_SETTINGS_BUTTON=false
